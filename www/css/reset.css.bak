}
.apply-footer>div.shenpiend>div:nth-child(2),.apply-footer>div.shenpistart>div:nth-child(2){
    background-color: white;
    border: 1px solid #efefef;
    margin-left: .6rem;
}
.apply-footer>div.shenpiend>div:nth-child(2)>a,.apply-footer>div.shenpistart>div:nth-child(2)>a{
     color: #ec0404;
 }


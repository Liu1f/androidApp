/* CSS Document */
* { margin: 0; padding: 0;font-size: .3rem;}
body,form,input{ margin: 0px; padding: 0px; border:0; font-size: 14px; font-weight: normal; font-family:"Microsoft Yahei"; color:#000;}
ul,ol,li { list-style: none; list-style-type:none; margin:0; padding:0px;}
p,div,a,h1,h2,h3,h4,h5,h6,span,ul,li,img,form,input,br,dl,dt,dd{ padding:0; margin:0;}
img {border-style:none; border:0; padding:0; margin:0;}
a{color:#000; text-decoration:none;}
a:hover{ text-decoration:none;}
.clearfix{ clear:both;  display:block;overflow: hidden;}
h1,h2,h3,h4,h5,h6{ font-weight:normal;}
em,i,samp,b{ font-style:normal;font-weight: normal; font-family:"Microsoft Yahei";}
body{background:#fff}

html { -webkit-text-size-adjust:none;overflow-x:hidden;}
body {min-width:300px;overflow-x:hidden;background-color: #efefef;}
.main{width:7.5rem;margin:0 auto;}
html, body { width: 100%;font-size:14px;   font-family: "Microsoft Yahei";}

.page-title{height:30px;padding-top:12px;font-weight:500;font-size:12px;}
.page-title span{display: inline-block;}
.page-title span:nth-child(1){width: 20%;text-align: left;padding-left:15px;}
.page-title span:nth-child(2){width: 50%;text-align: center;}
.page-title span:nth-child(3){width: 28%;text-align: right;}

.nav-input{
    border-bottom: 1px solid rgba(189, 189, 189, 0.39);
    display: flex;
    justify-content: center;
    align-items: center;
}
.nav-input input::-ms-input-placeholder{text-align: left;}
.nav-input input::-webkit-input-placeholder{text-align: left;}

.bx-bjcolore{
    background: #e8e8e8;
}
.bx-search{
    padding: .2rem;
}
.bx-search input{
    background: #ffffff;
    border: solid 1px #ececec;
    border-radius: 2px;
    outline: none;
    width: 100%;
    height: .6rem;
    font-size: .24rem;
    color: #666;
    text-align: center;
}
.sy-list{
    margin-bottom: .2rem;
}
.sy-list h3{
    padding: 0 .2rem ;
    font-size: .28rem;
    color: #333;
    line-height: .6rem;
}
.sy-list .list-ul{
    background-color: #fff;
    border: #666666;
    padding: .4rem 0 .2rem 0 ;
    border-radius: 2px;
}
.sy-list .list-ul ul li{
    float: left;
    width: 25%;
    text-align: center;
    margin-bottom: .2rem;
}
.sy-list .list-ul ul li img{
    width: .8rem;
}
.sy-list .list-ul ul li p{
    font-size: .24rem;
    color: #666;
    margin-top: .1rem;
}

.sy-nav{
    position: fixed;
    width: 7.5rem;
    left: 50%;
    margin-left: -3.75rem;
    bottom: 0;
    height: 1rem;
    padding-top: .1rem;
    background-color: #fff;
    border-top: solid 1px #e8e8e8;

}
.sy-nav .list-a{
    width: 50%;
    float: left;
    text-align: center;
}
.sy-nav .list-a img{
    width: .5rem
}
.sy-nav .list-a p{
    font-size: .26rem;
}
.sy-nav .list-a a.cur{
    color: #d81e06
}

.h-100{
    height: 1.2rem;
}

/* 登录页 */
.h-15{
    height: 100vh;
    background-image: url('../img/login-bg-image.png');
    background-size: 100% 100%;
    overflow: auto;
}
.login-top{
    height: 4.7rem;
    text-align: center;
    padding-top: 2rem;
    box-sizing: border-box;
    color: #6b6b6b;
}
.login-top>span{display: block;font-size: .35rem;margin-top: .3rem;}
.login-top>img{
    width: 1.38rem;
    height: 1.36rem;
}
.login-center{
    width: 6.8rem;
    height: 6.3rem;
    margin: 0 auto;
    padding-top: .6rem;
    background-color: #fff;
    box-sizing: border-box;
    text-align: center;
}
.login-center>input{
    width: 6rem;
    height: .85rem;
    margin:  .4rem auto 0;
    border-radius: 1.5rem;
    padding-left: .4rem;
    box-sizing: border-box;
    border: 1px solid #e9e9e9;
    font-size: .25rem;
    outline: none;
}
.login-center>input::placeholder{
    color: #bfbfca;
    font-size: .26rem!important;
}
.login-center>input:nth-child(3){
    width: .45rem;
    height: .45rem;
}
.login-center>div{
    width: 6rem;
    height: .45rem;
    margin: .25rem auto 0;
}
.login-center>div>div{
    height: .44rem;
    line-height: .44rem;
}
.login-center>div>div>input{
    width: .44rem;
    height: .44rem;
    border: none;
    background-color: #f6f4f6;
    color: #a2a1a2;
    font-size: .2rem;
    margin-right: .15rem;
    border-radius: .1rem;
}
.login{
    width: 6rem;
    height: .8rem!important;
    line-height: .8rem;
    background-color: #f24848;
    border-radius: 1.5rem;
    margin-top: .55rem;
}
.login>a{
    color: #fff;
}
